{
  "name": "lanchpad-backend",
  "version": "1.0.0",
  "description": "Lanchpad backend application",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --watch . --ext js,json --ignore node_modules/ --delay 500ms app.js",
    "dev-fast": "nodemon -e js,json --ignore node_modules/ app.js",
    "serve": "node dist/app.js",
    "prod": "npm run build && npm run serve",
    "test": "jest --verbose",
    "test:watch": "jest --watch"
    
  },
  "author": "Isreal",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.2",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "supertest": "^6.0.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "backend",
    "express",
    "mongodb",
    "authentication"
  ]
}